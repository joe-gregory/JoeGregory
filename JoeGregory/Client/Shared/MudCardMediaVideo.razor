@inherits MudComponentBase

<div class="video-container">
    <iframe class="responsive-iframe" src="@VideoUrl" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
</div>

@code {
    [Parameter]
    public string VideoUrl { get; set; }
}
